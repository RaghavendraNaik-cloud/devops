pipeline{
    agent any
        tools {
            maven 'Maven'
            jdk 'java_11'
        }
        stages{

        stage("git checkout"){
            steps{
                git branch: 'main', url: 'https://github.com/RaghavendraNaik-cloud/My-cloud.git' 
            }
        }
        stage ('Maven clean'){
            steps{
                sh 'mvn clean'
            }
        }
        stage('Maven build'){
            steps{
                sh 'mvn build'
            }
        }

    }
}

    


    
